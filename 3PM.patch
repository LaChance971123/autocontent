+LOG_COLORS = {
+    logging.INFO: "\033[36m",    # Cyan
+    logging.WARNING: "\033[33m", # Yellow
+    logging.ERROR: "\033[31m",   # Red
+}
+
+
+class ErrorCode(str, Enum):
+    SCRIPT_NOT_FOUND = "SCRIPT_NOT_FOUND"
